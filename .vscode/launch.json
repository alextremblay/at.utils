    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module: test",
            "type": "python",
            "request": "launch",
            "module": "si_utils.test",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Module: __init__",
            "type": "python",
            "request": "launch",
            "module": "si_utils.__init__",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
    ]
}
